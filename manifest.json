{
  "manifest_version": 3,
  "name": "Healthy man",
  "version": "0.1",
  "description": "A porn blocker extension",
  "action": {
    "default_popup": "src/components/popup/popup.html",
    "default_icon": "assets/icon.png"
  },
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/components/safe-page/safe.html",
        "src/components/safe-page/safe.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "src/background/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content/contentScript.js"],
      "type": "module"
    }
  ]
}